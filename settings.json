{

    "workbench.startupEditor": "none",
    "editor.fontFamily": "JetBrains Mono, Menlo, Monaco, Courier New, monospace",
    "editor.fontWeight": "normal",
    "editor.fontSize": 14,
    "workbench.editor.showTabs": "none",
    "breadcrumbs.filePath": "off",
    "breadcrumbs.enabled": false,
    "editor.lineHeight": 1.5,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "always",
    },
    "editor.formatOnSave": true,
    "editor.letterSpacing": 0,
    "debug.console.fontSize": 12,
        "http.proxySupport": "off",
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "terminal.integrated.localEchoEnabled": "off",
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    },
    "window.commandCenter": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "solid",
    "editor.quickSuggestionsDelay": 0,
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "editor.cursorStyle": "line",
    "workbench.preferredDarkColorTheme": "Material Theme Ocean",
    "workbench.preferredLightColorTheme": "Monospace Light",
    "workbench.iconTheme": "symbols",
    "window.autoDetectColorScheme": true,
    "workbench.layoutControl.enabled": false,
    "inlineChat.mode": "preview",
    "errorLens.borderRadius": "0.6rem",
    "errorLens.codeLensTemplate": "$source $severity $message",
    "errorLens.gutterIconSet": "borderless",
    "errorLens.statusBarMessageType": "closestSeverity",
    "errorLens.statusBarMessageEnabled": true,
    "errorLens.statusBarMessageTemplate": "ERROR: $message",
    "errorLens.statusBarMessageAlignment": "right",
    "errorLens.statusBarIconsAtZero": "hide",
    "errorLens.scrollbarHackEnabled": true,
    "errorLens.messageBackgroundMode": "message",
    "errorLens.padding": "5px 10px",
    "workbench.productIconTheme": "macos-modern",
    "symbols.hidesExplorerArrows": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.bracketSameLine": true,
}
